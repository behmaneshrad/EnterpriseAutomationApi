      "fullScopeAllowed": false
    }
  ],
  "clientScopes": [
    {
      "name": "roles-mapper",
      "protocol": "openid-connect",
      "protocolMappers": [
        {
          "name": "realm roles",
          "protocol": "openid-connect",
          "protocolMapper": "oidc-usermodel-realm-role-mapper",
          "consentRequired": false,
          "config": {
            "claim.name": "realm_access.roles",
            "jsonType.label": "String",
            "multivalued": "true",
            "access.token.claim": "true",
            "id.token.claim": "true"
          }
        },
        {
          "name": "client roles (enterprise-api)",
          "protocol": "openid-connect",
          "protocolMapper": "oidc-usermodel-client-role-mapper",
          "consentRequired": false,
          "config": {
            "client.id": "enterprise-api",
            "claim.name": "resource_access.enterprise-api.roles",
            "jsonType.label": "String",
            "multivalued": "true",
            "access.token.claim": "true",
            "id.token.claim": "true"
          }
        },
        {
          "name": "realm-management roles",
          "protocol": "openid-connect",
          "protocolMapper": "oidc-usermodel-client-role-mapper",
          "consentRequired": false,
          "config": {
            "client.id": "realm-management",
            "claim.name": "resource_access.realm-management.roles",
            "jsonType.label": "String",
            "multivalued": "true",
            "access.token.claim": "true",
            "id.token.claim": "true"
          }
        }
      ]
    },
    {
      "name": "aud-enterprise-api",
      "protocol": "openid-connect",
      "attributes": {
        "display.on.consent.screen": "true",
        "consent.screen.text": "Access enterprise-api",
        "include.in.token.scope": "true"
      },
      "protocolMappers": [
        {
          "name": "aud-mapper",
          "protocol": "openid-connect",
          "protocolMapper": "oidc-audience-mapper",
          "consentRequired": false,
          "config": {
            "included.client.audience": "enterprise-api",
            "id.token.claim": "true",
            "access.token.claim": "true"
          }
        }
      ]
    }
  ],
  "users": [
